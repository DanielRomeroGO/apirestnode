{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/api/alumnos(.*)",
            "dest": "/api/alumnos$1"
        },
        {
            "src": "/api/cursos(.*)",
            "dest": "/api/cursos$1"
        },
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}